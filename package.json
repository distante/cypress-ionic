{
  "name": "@saninn/cypress-ionic",
  "version": "1.0.0",
  "description": "A set of functions to help you interact with Ionic Framework elements in your Cypress tests",
  "main": "dist/index.js",
  "scripts": {
    "serve": "node ./scripts/serve.mjs",
    "develop": "start-server-and-test serve http://localhost:3999 test-open",
    "prepare": "husky install",
    "test-open": "cypress open",
    "test": "cypress run",
    "full-test": "start-server-and-test serve http://localhost:3999 test",
    "ci": "node --experimental-json-modules ./scripts/test.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distante/cypress-ionic.git"
  },
  "bugs": {
    "url": "https://github.com/distante/cypress-ionic/issues"
  },
  "keywords": [
    "cypress",
    "ionic"
  ],
  "author": "Saninn Salas Diaz",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@compodoc/live-server": "^1.2.2",
    "@testing-library/cypress": "^8.0.2",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "chalk": "^5.0.0",
    "currentIonicVersion": "npm:@ionic/core@^6.0.0",
    "cypress": "^9.1.1",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^25.3.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "start-server-and-test": "^1.14.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@ionic/core": ">= 5.0.0",
    "@testing-library/cypress": ">= 7.0.0",
    "cypress": ">= 7.0.0"
  },
  "oldSupportedVersions": {
    "ionic5": "npm:@ionic/core@^5.0.0"
  }
}
